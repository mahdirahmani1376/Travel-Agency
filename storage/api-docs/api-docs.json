    "components": {
        "schemas": {
            "LoginData": {
                "title": "Login data",
                "description": "login Data",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "name": {
                        "title": "email",
                        "description": "email of the user",
                        "type": "string",
                        "example": "test@example.com"
                    },
                    "password": {
                        "title": "password",
                        "description": "password of the User",
                        "type": "string",
                        "example": "test"
                    }
                },
                "type": "object"
            },
            "TourData": {
                "title": "Tour data",
                "description": "tour Data",
                "required": [
                    "name",
                    "starting_date",
                    "ending_date",
                    "price"
                ],
                "properties": {
                    "name": {
                        "title": "name",
                        "description": "name of the new Travel",
                        "type": "string",
                        "example": "test"
                    },
                    "starting_date": {
                        "title": "starting_date",
                        "description": "starting_date of the new Tour",
                        "type": "string",
                        "example": "2023_01_01"
                    },
                    "ending_date": {
                        "title": "ending_date",
                        "description": "nending_date of the new Tour",
                        "type": "integer",
                        "example": "2023_01_01"
                    },
                    "price": {
                        "title": "price",
                        "description": "price of the new Tour",
                        "type": "integer",
                        "example": "100"
                    }
                },
                "type": "object"
            },
            "TravelData": {
                "title": "Travel data",
                "description": "travel Data",
                "required": [
                    "is_public",
                    "name",
                    "description",
                    "number_of_days"
                ],
                "properties": {
                    "is_public": {
                        "title": "is_public",
                        "description": "is_public of the new Travel",
                        "type": "boolean",
                        "example": "true"
                    },
                    "name": {
                        "title": "name",
                        "description": "name of the new Travel",
                        "type": "string",
                        "example": "test"
                    },
                    "description": {
                        "title": "description",
                        "description": "description of the new Travel",
                        "type": "string",
                        "example": "test"
                    },
                    "number_of_days": {
                        "title": "number_of_days",
                        "description": "number_of_days of the new Travel",
                        "type": "integer",
                        "example": "1"
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "sanctum": {
                "type": "apiKey",
                "description": "Enter token in format (Bearer <token>)",
                "name": "Authorization",
                "in": "header"
            }
        }
    },
    "tags": [
        {
            "name": "Travel Agency",
            "description": "API Endpoints of Travel Agency"
