    "paths": {
        "/api/login": {
            "post": {
                "tags": [
                    "Users"
                ],
                "description": "login the user",
                "operationId": "LoginUser",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/LoginData"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "unauthorized"
                    }
                }
            }
        },
        "/api/travels/{travel}/tours": {
            "get": {
                "tags": [
                    "Tours"
                ],
                "summary": "Get list of tours",
                "description": "Returns list of tours",
                "operationId": "getTravelToursList",
                "parameters": [
                    {
                        "name": "travel",
                        "in": "path",
                        "description": "slug of the Travel",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "test"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthenticated",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        },
        "/api/{travel}/tours": {
            "post": {
                "tags": [
                    "Tours"
                ],
                "description": "store new Tour",
                "operationId": "storePartDelivery",
                "parameters": [
                    {
                        "name": "travel",
                        "in": "path",
                        "description": "travel_id of the Travel",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": "1"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/TourData"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerManager": []
                    }
                ]
            }
        },
        "/api/travels": {
            "get": {
                "tags": [
                    "Travels"
                ],
                "summary": "Get list of travels",
                "description": "Returns list of travels",
                "operationId": "getTravelsList",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthenticated",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "TravelParts"
                ],
                "description": "store new travel request",
                "operationId": "storeTravel",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/TravelData"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerManager": []
                    }
                ]
            }
        },
        "/api/travels/{travel}/update": {
            "put": {
                "tags": [
                    "TravelParts"
                ],
                "description": "update delivery part",
                "operationId": "updateTravel",
                "parameters": [
                    {
                        "name": "travel",
                        "in": "path",
                        "description": "travel_id of the Travel",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/TravelData"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerManager": []
                    }
                ]
            }
        }
